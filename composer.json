{
  "name": "jmiy/etcd-php",
  "type": "library",
  "description": "PHP client for Etcd v3",
  "keywords": [
    "etcdv3",
    "etcd",
    "etcd-client",
    "etcd-php"
  ],
  "homepage": "https://github.com/Jmiy/etcd-php-v3",
  "license": "MIT",
  "authors": [
    {
      "name": "Jmiy",
      "email": "jmiycen@gmail.com",
      "homepage": "https://github.com/Jmiy"
    }
  ],
  "require": {
    "php": ">= 7.2",
    "guzzlehttp/guzzle": "^7.0.1"
  },
  "require-dev": {
    "phpunit/phpunit": "4.5.*"
  },
  "autoload": {
    "psr-4": {
      "Etcd\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Etcd\\Tests\\": "tests/"
    }
  },
  "suggest": {
    "guzzlehttp/guzzle": "Required to use the Mailgun mail driver (^7.0.1)."
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.x-dev"
    }
  }
}
